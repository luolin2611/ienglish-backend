<configuration>
    <appender name="FILE" class="ch.qos.logback.core.FileAppender">
        <file>logs/log.log</file> <!-- 日志输出文件路径 -->
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern> <!-- 日志输出格式 -->
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="FILE"/>
    </root>
</configuration>
